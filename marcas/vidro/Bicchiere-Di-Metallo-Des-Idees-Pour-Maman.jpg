				$(".share-baloon").show();
			});

			$(".balloon-button").bind("mouseleave",function () {
				$(".share-baloon").hide();
			});
			$(".share-baloon").bind("mouseenter",function () {
				$(".share-baloon").show();
			});

			$(".share-baloon").bind("mouseleave",function () {
				$(".share-baloon").hide();
			});

			// Fleche Haut et Bas
			var scroll = true,
			      $arrowTop = $("#goTop"),
			      $arrowBot = $("#goBot"),
			      $window = $(window);

			 var checkScroll = function() {
			 	// If page don't need to be scroll
			 	if ($("body").height() < $(window).height()) {
			 	        $arrowTop.hide();
				        $arrowBot.hide();
				        return;
				    }

			 	if ($window.scrollTop() !== 0 && scroll === true) {
					$arrowTop.fadeIn();
				} else {
					$arrowTop.fadeOut();
				}

				if ($window.scrollTop() + 100 < $("html, body").height() && scroll === true) {
					$arrowBot.fadeIn();
				} else {
					$arrowBot.fadeOut();
				}
			 }

			$window.on('scroll', function(e) {
				checkScroll();
			});

			$arrowTop.click(function(event) {
				var scroll = !scroll;
				$("html,body").animate({scrollTop: $("html, body").offset().top}, 2000,'easeInOutCubic', function(event) {
					var scroll = !scroll;
					checkScroll();
				});
				event.stopPropagation();
			});

			$arrowBot.click(function(event) {
				var scroll = !scroll;
				$("html,body").animate({scrollTop: $("html, body").height() }, 2000,'easeInOutCubic', function(event) {
					var scroll = !scroll;
					checkScroll();
				});
				event.stopPropagation();
			});

			checkScroll();
	 	});
		
	</script>


